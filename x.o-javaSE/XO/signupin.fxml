<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="500.0" style="-fx-background-color: #009688;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="SignupinController">
  <children>
    <Button fx:id="signin" layoutX="101.0" layoutY="266.0" mnemonicParsing="false" onAction="#signinAction" prefHeight="59.0" prefWidth="284.0" text="sign in ">
      <font>
        <Font name="Comic Sans MS" size="25.0" fx:id="x1" />
      </font>
      <textFill>
        <Color blue="0.031" green="0.388" red="1.000" fx:id="x2" />
      </textFill>
    </Button>
    <Button fx:id="sign_up" font="$x1" layoutX="101.0" layoutY="171.0" mnemonicParsing="false" onAction="#sign_upAction" prefHeight="59.0" prefWidth="284.0" text="sign up" textFill="$x2" />
    <Button fx:id="back" layoutX="20.0" layoutY="20.0" mnemonicParsing="false" onAction="#backAction" prefHeight="28.0" prefWidth="81.0" style="-fx-background-color: #b7b7b7;" text="back" textFill="$x2">
      <font>
        <Font size="20.0" />
      </font>
    </Button>
    <Label layoutX="122.0" layoutY="55.0" minHeight="52.0" prefHeight="77.0" prefWidth="257.0" text="Registeration" textFill="$x2">
      <font>
        <Font name="Comic Sans MS Bold" size="36.0" />
      </font>
    </Label>
  </children>
  <effect>
    <ColorAdjust hue="0.04">
      <input>
        <InnerShadow choke="0.36" color="#00665a" height="255.0" radius="127.0" width="255.0" />
      </input>
    </ColorAdjust>
  </effect>
</AnchorPane>
